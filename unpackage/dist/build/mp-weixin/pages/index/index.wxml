<view class="container"><view style="margin-top:-150rpx;padding-top:150rpx;"><view class="animation-view"><view data-event-opts="{{[['tap',[['chooseLocation',['$event']]]]]}}" class="location" bindtap="__e"><myicon class="icon" vue-id="8dd740cc-1" icon="dingwei" bind:__l="__l"></myicon><text class="city">{{position}}</text></view><view data-event-opts="{{[['tap',[['doTTS',['$event']]]]]}}" class="toUpdate" bindtap="__e">语音播放</view><view class="center-container"><view class="flex-content-left"><block wx:if="{{todayData.iconTypeBai}}"><view class="today-weather-view"><text style="text-align:center;font-size:32rpx;color:white;height:50rpx;">今 天</text><text style="text-align:center;font-size:32rpx;color:white;">{{''+todayData.dateTxt+''}}</text><view style="text-align:center;color:white;"><myicon class="icon" style="color:white;font-size:32rpx;" vue-id="8dd740cc-2" icon="{{todayData.iconTypeBai}}" bind:__l="__l"></myicon><text style="font-size:32rpx;">{{todayData.tmp_min+"/"+todayData.tmp_max}}</text></view></view></block></view><block wx:if="{{liveWeather.iconType}}"><view class="flex-content-center"><view class="temp-out-content"><text class="today-temp">{{liveWeather.fl}}</text><text class="temp-symbol">℃</text></view><myicon class="icon" vue-id="8dd740cc-3" icon="{{liveWeather.iconType}}" bind:__l="__l"></myicon><text class="today-temp-bottom">{{liveWeather.cond_txt}}</text><view class="temp-out-content__bottom"><view class="flex-center__left"><text class="home-text ">{{"湿度: "+liveWeather.hum+"%"}}</text><text class="home-text">{{"风力："+liveWeather.wind_sc+"级"}}</text></view><view class="flex-center__right"><view class="home-text out-scroll-text"><text class="scroll-text">{{warmPrompt}}</text></view></view></view></view></block><block wx:if="{{tomorrowData.iconTypeBai}}"><view class="flex-content-right"><view class="today-weather-view"><text style="text-align:center;font-size:32rpx;color:white;height:50rpx;">明 天</text><text style="text-align:center;font-size:32rpx;color:white;">{{''+tomorrowData.dateTxt+''}}</text><view style="text-align:center;color:white;"><myicon class="icon" style="color:white;font-size:32rpx;" vue-id="8dd740cc-4" icon="{{tomorrowData.iconTypeBai}}" bind:__l="__l"></myicon><text style="font-size:32rpx;">{{tomorrowData.tmp_min+"/"+tomorrowData.tmp_max}}</text></view></view></view></block></view><view class="all-day-list"><scroll-view class="scroll-view_x" style="overflow:hidden;width:auto;height:100%;" scroll-x="{{true}}"><block wx:for="{{everyHourData}}" wx:for-item="item" wx:for-index="__i0__" wx:key="time"><view class="all-day-list-item"><view class="day-list-item">{{item.time+"点"}}</view><view class="day-list-item"><myicon style="color:white;font-size:36rpx;" vue-id="{{'8dd740cc-5-'+__i0__}}" icon="{{item.iconType}}" bind:__l="__l"></myicon></view><view class="day-list-item">{{item.tmp+"°"}}</view></view></block></scroll-view></view></view><view class="one-week-list"><block wx:for="{{everyWeekData}}" wx:for-item="item" wx:for-index="__i1__" wx:key="weekday"><view class="one-week-list-item"><view class="week-list-item" style="font-size:28rpx;"><view>{{item.weekday}}</view><view>{{item.date}}</view></view><view class="week-list-item">{{item.cond_txt_d}}</view><view class="week-list-item"><myicon style="color:white;font-size:44rpx;" vue-id="{{'8dd740cc-6-'+__i1__}}" icon="{{item.iconTypeBai}}" bind:__l="__l"></myicon></view><view class="week-list-item">{{item.tmp_min+"~"+item.tmp_max+"°"}}</view><view class="week-list-item"><myicon style="color:white;font-size:44rpx;" vue-id="{{'8dd740cc-7-'+__i1__}}" icon="{{item.iconTypeYe}}" bind:__l="__l"></myicon></view><view class="week-list-item">{{item.cond_txt_n}}</view><view class="week-list-item" style="font-size:28rpx;"><view>{{item.wind_dir=='无持续风向'?'无':item.wind_dir}}</view><view>{{item.wind_sc+"级"}}</view></view></view></block></view><block wx:if="{{todayData.sr}}"><view class="live-index-view"><view class="live-index-item"><view class="live-index-item-left"><view class="live-index-top-txt"><text>日出</text></view><view class="live-index-bottom-txt"><text>{{todayData.sr}}</text></view></view><view class="live-index-item-right"><view class="live-index-top-txt"><text>日落</text></view><view class="live-index-bottom-txt"><text>{{todayData.ss}}</text></view></view></view><view class="live-index-item"><view class="live-index-item-left"><view class="live-index-top-txt"><text>降雨概率</text></view><view class="live-index-bottom-txt"><text>{{todayData.pop+"%"}}</text></view></view><view class="live-index-item-right"><view class="live-index-top-txt"><text>风向角度</text></view><view class="live-index-bottom-txt"><text>{{todayData.wind_deg+"°"}}</text></view></view></view><view class="live-index-item"><view class="live-index-item-left"><view class="live-index-top-txt"><text>风速</text></view><view class="live-index-bottom-txt"><text>{{todayData.wind_spd+''}}<text style="font-size:28rpx;">公里/小时</text></text></view></view><view class="live-index-item-right"><view class="live-index-top-txt"><text>能见度</text></view><view class="live-index-bottom-txt"><text>{{todayData.vis+''}}<text style="font-size:28rpx;">公里</text></text></view></view></view><view class="live-index-item"><view class="live-index-item-left"><view class="live-index-top-txt"><text>月升时间</text></view><view class="live-index-bottom-txt"><text>{{todayData.mr}}</text></view></view><view class="live-index-item-right"><view class="live-index-top-txt"><text>月落时间</text></view><view class="live-index-bottom-txt"><text>{{todayData.ms}}</text></view></view></view></view></block><block wx:if="{{lifeStyle.length!=0}}"><view class="last-view"><block wx:for="{{lifeStyle}}" wx:for-item="item" wx:for-index="__i2__" wx:key="type"><view class="last-view-item"><view class="last-view-item-top">{{lifeEnum[item.type]}}</view><view class="last-view-item-bottom">{{item.brf}}</view><view class="home-text out-scroll-text"><text class="scroll-text">{{item.txt}}</text></view></view></block></view></block></view></view>